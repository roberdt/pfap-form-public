                "dev"
            ],
            "port": 9229,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Next.js: Build",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "build"
            ],
            "port": 9229,
